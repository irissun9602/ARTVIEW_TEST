<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
             "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="net.skhu.user.ApplicationUserMapper">

  <select id="findByUsername" resultType="ApplicationUser">
    SELECT * FROM user WHERE login_id = #{login_id}
  </select>
  <select id="findAll" resultType="ApplicationUser">
    SELECT * FROM user
  </select>
  
    <insert id="save" useGeneratedKeys="true" keyProperty="id">
    INSERT user (login_id,pwd, name, phone, email,age)
    VALUES (#{login_id},#{pwd}, #{name}, #{phone}, #{email},#{age})
  </insert>
</mapper>

